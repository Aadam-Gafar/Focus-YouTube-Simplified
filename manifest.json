{
    "manifest_version": 3,
    "name": "TrueTube",
    "version": "1.0.0",
    "description": "A less addicting version of YouTube.",
   
    "content_scripts": [{
      "css": ["styles.css"],
      "js": ["content.js"],
      "matches": ["https://www.youtube.com/*"]
    }],

    "icons": {
      "128": "icon.png"
    },

    "web_accessible_resources": [
      {
        "resources": ["welcome.png"],
        "matches": ["https://www.youtube.com/*"]
      }
    ]
  }